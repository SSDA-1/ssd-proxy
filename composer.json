{
    "name": "ssda1/proxies",
    "description": "proxies",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "*",
        "ssda-1/proxies": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": "ssda1\\proxies\\Providers\\ProxyServiceProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "ssda1\\proxies\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "@php artisan migrate --path=vendor/ssda1/proxies/database/migrations"
        ]
    }
}
