{
    "name": "ssda1/proxies",
    "description": "proxies",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ssda1\\proxies\\Providers\\ProxyServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ssda1\\proxies\\": "src/"
        },
        "files": [
            "src/Providers/ProxyServiceProvider.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"ssda1\\proxies\\Providers\\ProxyServiceProvider\" --tag=config",
            "php vendor/ssda-1/proxies/post-install.php",
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": "php vendor/ssda-1/proxies/post-install.php"
    }
}
